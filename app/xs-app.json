{
    "welcomeFile": "index.html",
    "authenticationMethod": "none",
    "routes": [
        {
            "source": "^/error-service/(.*)$",
            "target": "/error-service/$1",
            "destination": "srv-api",
            "authenticationType": "none"
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "localDir": ".",
            "authenticationType": "none"
        }
    ]
}